{
  "name": "ci",
  "version": "0.0.2",
  "description": "Simple Continuous Integration",
  "main": "src/app.js",
  "scripts": {
    "babel-node": "babel-node --presets=/*a*/ ",
    "clean": "rm -rf dist && mkdir dist",
    "start": "nodemon --exec babel-node -- ./src/app.js",
    "debug": "nodemon --inspect-brk --exec babel-node -- ./src/app.js",
    "build": "babel -d dist src -s --copy-files",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "Oskar Lindgren <lindgren.oskar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "logform": "^1.10.0",
    "morgan": "^1.9.1",
    "rimraf": "^2.7.1",
    "winston": "^3.2.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^1.19.2"
  },
  "snyk": true
}
